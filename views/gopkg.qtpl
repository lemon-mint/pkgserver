{% func GoPackage(pkg, vcs, url string) %}
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<meta name="go-import" content="{%s pkg %} {%s vcs %} {%s= url %}">
<meta http-equiv="refresh" content="0; url=https://pkg.go.dev/{%s pkg %}">
</head>
<body>
<a href="https://pkg.go.dev/{%s pkg %}">Redirecting to documentation...</a>
</body>
</html>
{% endfunc %}
